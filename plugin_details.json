            "activation_modes": [
              "llm_judge",
              "keyword"
            ],
            "keywords": [
              "搜索",
              "查询",
              "查找",
              "search",
              "find"
            ]
          }
        ],
        "features": [
          "智能LLM判定生成时机",
          "高效AI搜索功能",
          "结果缓存机制",
          "完整的错误处理"
        ]
      }
    }
